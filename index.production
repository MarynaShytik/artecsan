<!DOCTYPE html>
<html ng-app="inspinia">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Page title set in pageTitle directive -->
    <title page-title>Artecsan powered by Skrible</title>

    <!-- Bootstrap -->
    <link href="css/bootstrap.min.css?ver=@@hash" rel="stylesheet">

    <!-- Font awesome -->
    <link href="font-awesome/css/font-awesome.css?ver=@@hash" rel="stylesheet">

    <!-- Plugins -->
    <link href="css/plugins/sweetalert/sweetalert.css?ver=@@hash" rel="stylesheet">

    <!-- Main Inspinia CSS files -->
    <link href="css/animate.css?ver=@@hash" rel="stylesheet">
    <link id="loadBefore" href="css/style.css?ver=@@hash" rel="stylesheet">

    <link href="css/Artecsan.css?ver=@@hash" rel="stylesheet">

</head>

<!-- ControllerAs syntax -->
<!-- Main controller with serveral data used in Inspinia theme on diferent view -->
<body ng-controller="MainCtrl as main" class="{{$state.current.data.specialClass}}" landing-scrollspy id="page-top">

<!-- Main view  -->
<div ui-view></div>

<!-- jQuery and Bootstrap -->
<script src="js/jquery/jquery-2.1.1.min.js?ver=@@hash"></script>
<script src="js/plugins/jquery-ui/jquery-ui.js?ver=@@hash"></script>
<script src="js/bootstrap/bootstrap.min.js?ver=@@hash"></script>

<!-- MetisMenu -->
<script src="js/plugins/metisMenu/jquery.metisMenu.js?ver=@@hash"></script>

<!-- SlimScroll -->
<script src="js/plugins/slimscroll/jquery.slimscroll.min.js?ver=@@hash"></script>

<!-- Peace JS -->
<script src="js/plugins/pace/pace.min.js?ver=@@hash"></script>

<!-- Custom and plugin javascript -->
<script src="js/inspinia.js?ver=@@hash"></script>

<!-- Main Angular scripts-->
<script src="js/angular/angular.min.js?ver=@@hash"></script>
<script src="js/angular/angular-sanitize.min.js?ver=@@hash"></script>
<script src="js/plugins/oclazyload/dist/ocLazyLoad.min.js?ver=@@hash"></script>
<script src="js/ui-router/angular-ui-router.min.js?ver=@@hash"></script>
<script src="js/bootstrap/ui-bootstrap-tpls-1.1.2.min.js?ver=@@hash"></script>
<script src="js/plugins/angular-idle/angular-idle.js?ver=@@hash"></script>
<script src="js/plugins/datetime-picker.js?ver=@@hash"></script>


<!-- Thirty part libs -->
<script src="js/plugins/angular-local-storage.min.js?ver=@@hash"></script>
<script src="js/plugins/sweetalert/sweetalert.min.js?ver=@@hash"></script>
<script src="js/plugins/sweetalert/angular-sweetalert.min.js?ver=@@hash"></script>

<!-- Angular App Script -->
<script src="dist/app.js?ver=@@hash"></script>


<!-- main controllers -->
<script src="js/controllers/homeMenuController.js?ver=@@hash"></script>

<!-- services -->
<script src="dist/services.js?ver=@@hash"></script>

<!-- directives -->
<script src="dist/directives.js?ver=@@hash"></script>

<!-- components -->
<script src="dist/components.js?ver=@@hash"></script>


<!-- filters -->
<script src="dist/filters.js?ver=@@hash"></script>


<script src='https://www.google.com/recaptcha/api.js'></script>
<script src='https://cdn.jsdelivr.net/g/lodash@4(lodash.min.js+lodash.fp.min.js)'></script>
</body>
</html>